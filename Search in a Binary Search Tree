class Solution {
public:
    TreeNode* searchBST(TreeNode* root, int val) {
        while(root != nullptr){
            if(val == root->val){
                return root;
            }
            else if( val > root->val){
                root = root->right;
            }else{
                root = root->left;
            }
        }
        return nullptr;
    }
};
